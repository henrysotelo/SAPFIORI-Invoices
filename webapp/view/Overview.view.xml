<mvc:View 
    controllerName="dev.invoices.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:core="sap.ui.core" 
    displayBlock="true" >
    <Page id="page" title="{i18n>title}">

    <landmarkInfo>
        <PageAccessibleLandmarkInfo 
            id="_IDGenPageAccessibleLandmarkInfo1" 
            rootRole="Region"
            rootLabel="{i18n>overviewRootLabel}" 
            contentRole="Main" 
            contentLabel="{i18n>overviewContetetLabel}" 
            headerRole="Banner"
            headerLabel="{i18n>overviewteaderLabely}"/>
    </landmarkInfo>
        <headerContent>
            <Button id="_helloDialogHeader"
                        icon="sap-icon://action"
                        press=".onOpenDialogHeader"
                        class="sapUiSmallMarginEnd" />
        </headerContent>
            <content>
                <mvc:XMLView id="_IDGenXMLView1" viewName="dev.invoices.view.HelloPanel" />
                <mvc:XMLView id="_IDGenXMLView2" viewName="dev.invoices.view.InvoicesList" />
            </content>
    </Page>
</mvc:View>